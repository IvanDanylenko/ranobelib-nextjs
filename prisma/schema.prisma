// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:../ranobelib.sqlite.db"
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id Int @id @default(autoincrement())
  email String
  password String
}